{
  "name": "Ad Inspector",
  "version": "2.0",
  "manifest_version": 2,
  "author": "Florin Alexandrescu",
  "description": "Shows ad parameters from ads on the current page",
  "devtools_page" : "devtools.html",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}